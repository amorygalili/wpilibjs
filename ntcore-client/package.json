{
  "name": "ntcore-client",
  "version": "1.0.0",
  "description": "NetworkTables 4 client for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run build",
    "example:basic": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/basic-usage.ts",
    "example:advanced": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/advanced-usage.ts",
    "example:node": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/node-example.ts",
    "example:ntcore": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/ntcore-api-example.ts",
    "example:simple-ntcore": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/simple-ntcore-example.ts",
    "example:outlineviewer": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/outlineviewer-test.ts",
    "example:connection-test": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/connection-test.ts"
  },
  "keywords": ["networktables", "wpilib", "frc", "robotics"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@msgpack/msgpack": "3.0.0-beta2",
    "ws": "^8.14.2",
    "node-fetch": "^3.3.2"
  }
}

