{
  "name": "@wpilibjs/wpilibts",
  "version": "0.1.0",
  "description": "WPILib implementation in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "example:simple-robot-sim": "node --loader ts-node/esm ./examples/SimpleRobotSimulation.ts",
    "example:simple-nt-test": "node --loader ts-node/esm ./examples/SimpleNTTest.ts"
  },
  "keywords": [
    "wpilib",
    "frc",
    "robotics",
    "typescript"
  ],
  "author": "WPILib",
  "license": "BSD-3-Clause",
  "dependencies": {
    "ws": "^8.18.1",
    "ntcore-client": "file:../ntcore-client"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/ws": "^8.18.1",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "ts-node": "^10.9.2"
  }
}
